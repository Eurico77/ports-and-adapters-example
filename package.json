  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.4",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/knex": "^0.16.1",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "knex": "^3.1.0",
    "pg": "^8.12.0"
  }
}
